#include "JNI/Types/Param.h"

using namespace nsJNI;
Param::Param(const string& type, const string& name)
{
	cout << "2.4.3.4.7.1" << endl;
	this->_type=type;
	this->_name = name;
}

Param::~Param()
{


}

void Param::convert()
{


}

std::string& Param::getName()
{
	return _name;
}

std::string& Param::getType()
{
	return _type;
}
